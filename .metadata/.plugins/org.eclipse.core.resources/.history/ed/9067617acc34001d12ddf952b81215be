package test;

public class evenoddarray {
public static void main(String[] args) {
	int[] arr =  new int[] {1,2,3,4,5,6,7,8,9};
	
	int[] evenarray = new int[arr.length/2];
	int[] oddarray = new int[arr.length/2];
	
	for(int ref:arr) {
		int i=0;
		i=i+1;
		if(ref%2==1) {
			oddarray[i]=ref;
		}
	}
	
}
}
